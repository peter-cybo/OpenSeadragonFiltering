{
    "extends": "./node_modules/eslint-config-defaults/configurations/google.js",
    "rules": {
        "indent": [
            2,
            4
        ],
        "newline-after-var": 0,
        "dot-location": [
            2,
            "property"
        ],
        "space-before-function-paren": "off",
        "space-before-blocks": "off",
        "eslint-plugin-unicorn/explicit-length-check": "off"
    },
    "parserOptions": {
        "ecmaVersion": 2017
    },
    "globals": {
        "window": true,
        "define": true,
        "require": true,
        "Caman": true
    }
}